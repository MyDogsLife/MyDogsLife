<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.dogslife"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light" >
        <activity android:name="com.example.user.dogslife.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.user.dogslife.MenuTab" />
        <activity android:name="com.example.user.dogslife.Camera" />
        <activity android:name="com.example.user.dogslife.Diary" />
        <activity
            android:name="com.example.user.dogslife.Write"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name="com.example.user.dogslife.Information"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
    </application>

</manifest>